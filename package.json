{
  "name": "vue-error-capture",
  "type": "module",
  "version": "1.0.2",
  "private": false,
  "description": "前端 Vue 3 錯誤捕捉工具，支援全域 console、Vue、XHR、fetch 等錯誤攔截",
  "author": "Whiskas",
  "license": "MIT",
  "homepage": "https://github.com/cat94cute/vue-error-capture#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/cat94cute/vue-error-capture"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --project tsconfig.build.json",
    "preview": "vite preview",
    "type-check": "vue-tsc --build",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "dependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vue-tsc": "^3.1.4"
  }
}
